{
  "name": "cs-utils",
  "displayName": "Cheat Sheet Utils",
  "description": "Utilities for Cheat Sheet project",
  "version": "0.0.1",
  "author": "lamhq",
  "repository": {
    "type": "git",
    "url": "https://github.com/lamhq/cheat-sheet-utils.git"
  },
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cs-utils.openInBrowser",
        "title": "Open file in browser"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "cs-utils.openInBrowser",
          "group": "navigation",
          "when": "editorTextFocus"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "pnpm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.109.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "eslint": "^9.39.2",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
